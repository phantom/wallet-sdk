# Phantom Server SDK Configuration
# Copy this file to .env and fill in your values

# Your Phantom organization ID
ORGANIZATION_ID=your-organization-id

# Your Phantom App ID
APP_ID=your-app-id

# Your Phantom Organization Private Key (base58 encoded, P256 private key)
ORGANIZATION_PRIVATE_KEY=your-base58-encoded-private-key

# Phantom Wallet API URL
WALLET_API=https://staging-api.phantom.app/v1/wallets

# Solana RPC URL (defaults to devnet, can be changed to mainnet)
SOLANA_RPC_URL=https://api.devnet.solana.com

# Network to use for demo (devnet or mainnet)
NETWORK=devnet 